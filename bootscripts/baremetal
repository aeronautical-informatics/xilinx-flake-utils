echo ======================================================
echo ======== baremetal for %project on %platform =========
echo ======================================================
echo ------- settings -------
setenv image_path /srv/tftp/%project.%platform.%config.elf
setenv fetch_image 'tftpboot ${loadaddr} ${image_path}'
setenv boot_image 'bootelf'

echo ------- EXECUTION ------
run fetch_image
run boot_image